# This analog device temlate is for use with the bkhAsyn support module.
#-----------------------------------------------------------------------
record( longin, "$(P):LI:A:$(MOD):SB$(N)"){
  field( DESC, "$(DESC)")
  field( DTYP, "asynInt32")
  field( INP,  "@asyn($(PORT),$(ADDR),1)LI_SBYTE")
  field( SCAN, "I/O Intr")
}
record( ai, "$(P):AI:A:$(MOD):CH$(N)"){
  field( DESC, "$(DESC)")
  field( DTYP, "asynInt32")
  field( INP,  "@asyn($(PORT),$(ADDR),1)AI_DATAIN")
  field( LINR, "LINEAR")
  field( EGU,  "$(EGU)")
  field( EGUL, "$(EGUL)")
  field( EGUF, "$(EGUF)")
  field( PREC, "$(PREC)")
  field( SCAN, "I/O Intr")
}
record( bo, "$(P):BO:A:$(MOD):CH$(N)"){
  field( DTYP, "asynInt32")
  field( OUT,  "@asyn($(PORT),$(ADDR),1)BO_READ")
  field( ZNAM, "off")
  field( ONAM, "on")
}
