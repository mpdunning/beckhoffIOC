# file: twoMotMove.db
# records for coordinated moving of two motors, either in the same direction
# or in opposite direction.  Always by the same amount.
#---------------------------------------------------------------------------
record( ao, "$(P):AO:$(MD):PRESET:REL:1"){
  field( DESC, "Relative:")
  field( DTYP, "Soft Channel")
  field( OMSL, "closed_loop")
  field( DOL,  "0.05")
  field( EGU,  "mm")
  field( PREC, "2")
  field( OUT,  "$(P):AO:$(MD):SET:REL:1 PP")
}
