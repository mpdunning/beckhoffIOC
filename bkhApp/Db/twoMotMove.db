record( ao, "$(P):AO:$(MD):SET:REL:1"){
  field( DESC, "Relative:")
  field( DTYP, "Soft Channel")
  field( EGU,  "mm")
  field( PREC, "2")
  field( PINI, "YES")
  field( FLNK, "$(P):DF:$(MD):REL:1")
}
record( dfanout, "$(P):DF:$(MD):REL:1"){
  field( DESC, "value for relative move")
  field( DOL,  "$(P):AO:$(MD):SET:REL:1")
  field( OMSL, "closed_loop")
  field( OUTA, "$(P):AO:$(M1):CH1:SET:INC:MM PP")
  field( OUTB, "$(P):AO:$(M2):CH1:SET:INC:MM PP")
}
record( dfanout, "$(P):DF:$(MD):REL:RL"){
