  field( DOL,  "1")
  field( OMSL, "closed_loop")
  field( OUTA, "$(P):BO:$(M1):CH1:JOG:FOR PP")
  field( OUTB, "$(P):BO:$(M2):CH1:JOG:REV PP")
}
